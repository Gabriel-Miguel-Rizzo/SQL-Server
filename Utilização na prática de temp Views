BEGIN

DECLARE @VARTeste1 varchar(50) ;
SET @VarTeste1 = 'Mensagem de erro';

select produto, COLECAO, LINHA, GRUPO_PRODUTO, MATERIAL
into #INFOS2023
FROM PRODUTOS
where YEAR(DATA_CADASTRAMENTO) = 2023;



select produto, COLECAO, LINHA, GRUPO_PRODUTO, MATERIAL
into #INFOS2024
FROM PRODUTOS
where YEAR(DATA_CADASTRAMENTO) = 2024;

select *
from #INFOS2023

UNION

SELECT *
FROM #INFOS2024;


PRINT @VarTeste1;
END;
